    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^3.0.6",
    "typescript": "^3.9.5",
    "ua-parser-js": "^1.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
