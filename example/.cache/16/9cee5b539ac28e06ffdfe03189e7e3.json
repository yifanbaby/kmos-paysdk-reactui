ight: 1px solid #F2F2F2;\n      li {\n        display: flex;\n        align-items: center;\n        padding: 15px 20px 15px 24px;\n        font-size: 15px;\n        &:first-child.active {\n          border-top: none;\n          padding-top: 15px;\n        }\n        &.active {\n          margin: 0 -1px 0 0;\n          background: #fff;\n          border-top: 1px solid #F2F2F2;\n          border-bottom: 1px solid #F2F2F2;\n          padding-top: 14px;\n          padding-bottom: 14px;\n        }\n        &:last-child.active {\n          border-bottom: none;\n          padding-bottom: 15px;\n        }\n        // span {\n        //   margin-left: 10px;\n        // }\n        .inherit {\n          color: #dfdfdf;\n          font-size: 20px;\n          &.active {\n            color: #025EFF;\n          }\n        }\n        .checked-icon {\n          display: block;\n          width: 20px;\n          height: 20px;\n        }\n        img.pay-icon {\n          margin: 0 15px 0 20px;\n          width: 29px;\n          height: 29px;\n        }\n      }\n    }\n  }\n  .pay-wechat {\n    // margin-left: 350px;\n    // padding-top: 35px;\n    font-size: 14px;\n    text-align: center;\n    display: flex;\n    align-items: center;\n    flex: 1;\n    .wechat-container {\n      width: 170px;\n      margin: 0 auto;\n    }\n    .apply-close {\n      margin: 0 auto;\n    }\n    .wechat-wrapper {\n      margin-top: 10px;\n      margin-bottom: 10px;\n      height: 135px;\n      position: relative;\n      .pay-modal {\n        position: absolute;\n        top: 0;\n        left: 17px;\n        width: 136px;\n        height: 136px;\n        opacity: 0.8;\n        background: #000;\n        // border-radius: 10px;\n      }\n    }\n    .refresh-btn {\n      margin: 13px auto 0;\n      width: 72px;\n      height: 28px;\n      line-height: 28px;\n      background: #025EFF;\n      border-radius: 14px;\n      font-size:12px;\n      color: #fff;\n      text-align: center;\n    }\n  }\n}\n.agreement-wrapper {\n  margin-top: 16px;\n  padding-left: 24px;\n  .checkout-icon {\n    position: relative;\n    top: 1px;\n    margin-right: 4px;\n    width: 12px;\n    height: 12px;\n  }\n  span {\n    font-size: 12px;\n    color: #333;\n    &.agreement-text {\n      color: #0077FF;\n    }\n  }\n}\n.loading {\n  position: absolute;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  background: #000;\n  opacity: 0.2;\n  z-index: 999;\n  text-align: center;\n  img {\n    position: relative;\n    top: 50%;\n    transform: translateY(-50%);\n  }\n}\n.apply-title {\n  margin-bottom: 10px;\n  font-size: 22px;\n  color: #222;\n  font-weight: 600;\n}"},"lineCount":null}},"error":null,"hash":"7f1ab5a0d3932aa7fc6588b1f801a5c8","cacheData":{"env":{}}}